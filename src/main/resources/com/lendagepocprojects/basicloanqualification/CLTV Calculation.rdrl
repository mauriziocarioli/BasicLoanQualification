package com.lendagepocprojects.basicloanqualification;

import java.lang.Number;

rule "CLTV Calculation"
	dialect "mvel"
	salience 1
	when
		lead : LeadInformation( value : propertyValue > 0 , debt : eligibleDebt, liability : totalMortgageLiability)
	then
		lead.setCLTV( 100*((debt+500*liability)/value) );
end
